class C {
    int x = 1;
    int y = 100;

    int addition(int op1,int op2){
        return op1 + op2;
    };

    int substraction(int op1,int op2){
        return op1 - op2;
    };

    boolean inferior(int op1,int op2){
        return op2 > op1;
    };

    void helloWorld(){
        writeln("Hello World");
    };

    main{
        write("addition : ");
        writeln(addition(x,y));
        write("substraction : ");
        writeln(substraction(x,y));
        write("inferior : ");
        writeln(inferior(x,y));
        helloWorld();
    }
}